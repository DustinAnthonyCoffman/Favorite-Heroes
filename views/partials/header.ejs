      <% function getRandom(min, max) { %>
        <% return Math.random() * (max - min) + min; } %>
     
     
     <% let randomNum = getRandom(1,731); %>
     <% let randomRound = Math.round(randomNum); %>
     
     
      <nav class="navbar fixed-top navbar-light bg-dark">
          <a class="btn btn-dark" href="/home">Favorite Hero</a>
         
          <form action="/heros/?_method=PUT" method="post"> 
            <input type="hidden" name="name" value="<%=randomRound%>"> </input>
          <button type="submit" class="btn btn-dark">Random Hero</button>
          </form>
          <a href="/favHeros"> <button type="button" class="btn btn-dark">Hero Collection</button> </a>
